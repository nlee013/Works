package com.day5assign;

import java.util.Scanner;

public class Day5Q1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int[] num = new int[5];
		int i, j, temp;
		
		System.out.print("Type any five numbers: ");
		
		for(i = 0; i< num.length; i++) {
		num[i] = sc.nextInt();
		
		}
		System.out.printf("%3d", num[i]);
	}

}
